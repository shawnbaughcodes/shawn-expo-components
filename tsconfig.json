{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "baseUrl": "src",
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "*": ["*"],
      "src": ["./src"]
    },
    "module": "es6",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "rootDir": ".",
    "strict": true
  }
}
